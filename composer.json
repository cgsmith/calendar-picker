{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.293",
        "blade-ui-kit/blade-heroicons": "^2.3",
        "filament/filament": "^3.0-stable",
        "filament/spatie-laravel-settings-plugin": "^3.0-stable",
        "guava/filament-icon-picker": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "jeffgreco13/filament-breezy": "~v2.4",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "sentry/sentry-laravel": "^4.7",
        "spatie/laravel-markdown": "^2.5",
        "spatie/laravel-settings": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "deployer/deployer": "^7.4",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php ./vendor/bin/pint --test --config=pint.json",
            "@php ./vendor/bin/phpstan --no-progress --error-format=raw",
            "@php ./vendor/bin/pest"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
